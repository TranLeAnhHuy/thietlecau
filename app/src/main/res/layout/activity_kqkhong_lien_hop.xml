<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".KQKhongLienHop">



    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:orientation="vertical">
            //hiển thị kết quả đặt trưng hình học
            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
                android:layout_marginTop="5dp">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Đặc trưng hình học"
                android:textAllCaps="true"
                android:textSize="20dp"
                android:textColor="#fb04069c"
                android:textStyle="bold" />
            </LinearLayout>

            //hiển thị kết quả đặt trưng hình học

            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Giai đoạn 1 - Mặt cắt nguyên"
                android:layout_marginTop="10dp"
                android:textSize="16dp"
                android:textColor="#fb04069c"
                android:textStyle="bold" />

              </LinearLayout>
            // diện tích
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="Diện tích mặt cắt nguyên"
                    android:textSize="16dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/txtAnc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:minWidth="84dp"
                    android:layout_marginLeft="36dp"
                    android:textColor="#fb04069c"
                    android:background="#f2f4f4"
                    android:textSize="20dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="mm2"
                    android:textSize="16dp" />

            </LinearLayout>
            //Momen tĩnh đối với đáy dầm
            <LinearLayout
