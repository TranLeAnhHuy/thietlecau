                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:minWidth="75dp"
                    android:layout_marginLeft="10dp"
                    android:textColor="#fb04069c"
                    android:background="#f2f4f4"
                    android:textSize="20dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="mm3"
                    android:textSize="16dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Giai đoạn 2 - Mặt cắt liên hợp dài hạn"
                    android:layout_marginTop="10dp"
                    android:textSize="16dp"
                    android:textColor="#fb04069c"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                >

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:stretchColumns="*"
                    >
                    <TableRow>
                        <TextView
                            android:text="ĐTHH"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:textStyle="bold"
                            android:gravity="center_horizontal"

                            />
                        <TextView
                            android:text="Dầm trong"
                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"
                            />
                        <TextView
                            android:text="Dầm ngoài"
                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:textStyle="bold"
                            android:gravity="center_horizontal"

                            />
                        <TextView
                            android:text="Đơn vị"
                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:textStyle="bold"
                            android:gravity="center_horizontal"
                            android:background="#b8d5f9"
                            />
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:text="Bs=Bi/n"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            android:textStyle="bold"
                            android:gravity="center_horizontal"

                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:id="@+id/txtBsDT"
                            android:gravity="center_horizontal"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:id="@+id/txtBsDN"
                            android:padding="5dp"
                            android:gravity="center_horizontal"
                            android:background="#dbe7f7"
                            />
                        <TextView
                            android:text="mm"
                            android:layout_marginLeft="2dp"
                            android:gravity="center_horizontal"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            />
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:text="Alt"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"

                            />
                        <TextView

