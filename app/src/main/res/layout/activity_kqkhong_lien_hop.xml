                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtAltDN"

                            />
                        <TextView
                            android:text="mm2"
                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            />
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:text="Sltt"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"

                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtSlttDT"
                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:gravity="center_horizontal"
                            android:background="#dbe7f7"
                            android:id="@+id/txtSlttDN"
                            />
                        <TextView
                            android:text="mm3"
                            android:layout_marginLeft="2dp"
                            android:gravity="center_horizontal"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            />
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:text="Yltd"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:textStyle="bold"
                            android:gravity="center_horizontal"

                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:gravity="center_horizontal"
                            android:background="#b8d5f9"
                            android:id="@+id/txtYltdDT"
                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtYltdDN"

                            />
                        <TextView
                            android:text="mm"
                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            />
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:text="Yltt"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"

                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtYlttDT"
                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:gravity="center_horizontal"
                            android:padding="5dp"
                            android:background="#dbe7f7"
                            android:id="@+id/txtYlttDN"
                            />
                        <TextView
                            android:text="mm"
                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:gravity="center_horizontal"
                            android:background="#dbe7f7"
                            />
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:text="Ilt"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            android:gravity="center_horizontal"
                            android:textStyle="bold"

                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:gravity="center_horizontal"
                            android:background="#b8d5f9"
                            android:id="@+id/txtIltDT"
                            />
                        <TextView

                            android:layout_marginLeft="2dp"
                            android:padding="5dp"
                            android:gravity="center_horizontal"
                            android:background="#b8d5f9"
                            android:id="@+id/txtIltDN"

                            />
                        <TextView
                            android:text="mm4"
                            android:layout_marginLeft="2dp"
                            android:gravity="center_horizontal"
                            android:padding="5dp"
                            android:background="#b8d5f9"
                            />
                    </TableRow>
                </TableLayout>
            </LinearLayout>
















        </LinearLayout>

    </ScrollView>

</LinearLayout>

